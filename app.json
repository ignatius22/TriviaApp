{
  "expo": {
    "name": "TriviaApp",
    "slug": "TriviaApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.iggycode.TriviaApp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-font",
        {
          "fonts": [
            "./assets/fonts/Space_Grotesk/SpaceGrotesk-Medium.ttf",
            "./assets/fonts/Space_Grotesk/SpaceGrotesk-Regular.ttf",
            "./assets/fonts/Space_Grotesk/SpaceGrotesk-Light.ttf",
            "./assets/fonts/Space_Grotesk/SpaceGrotesk-Bold.ttf",
            "./assets/fonts/Space_Grotesk/SpaceGrotesk-SemiBold.ttf"
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "aa974177-6d58-4c2f-9841-f16b0033b680"
      }
    }
  }
}
